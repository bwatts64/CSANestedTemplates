{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vmName": {
            "type": "string"
        },
        "configModuleURL": {
            "type": "string"
        },
        "configFunction": {
            "type": "string"
        },
        "lbIP": {
            "type": "string"
        },
        "acrName": {
            "type": "string"
        },
        "aksName": {
            "type": "string"
        },
        "gwName": {
            "type": "string"
        },
        "rgName": {
            "type": "string"
        },
        "saName": {
            "type": "string"
        },
        "aiKey": {
            "type": "string"
        },
        "sqlName": {
            "type": "string"
        },
        "dbName": {
            "type": "string"
        },
        "sqlAdmin": {
            "type": "string"
        },
        "sqlPwd": {
            "type": "securestring"
        },
        "saKey": {
            "type": "string"
        }
    },
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "name": "[concat(parameters('vmName'), '/dscConfig')]",
            "apiVersion": "2017-03-30",
            "location": "[resourceGroup().location]",
            "properties": {
                "publisher": "Microsoft.Powershell",
                "type": "DSC",
                "typeHandlerVersion": "2.71",
                "autoUpgradeMinorVersion": true,
                "settings": {
                    "modulesUrl": "[parameters('configModuleURL')]",
                    "configurationFunction": "[parameters('configFunction')]",
                    "properties": {
                        "lbIP": "[parameters('lbIP')]",
                        "acrName": "[parameters('acrName')]",
                        "aksName": "[parameters('aksName')]",
                        "gwName": "[parameters('gwName')]",
                        "rgName": "[parameters('rgName')]",
                        "saName": "[parameters('saName')]",
                        "aiKey": "[parameters('aiKey')]",
                        "sqlName": "[parameters('sqlName')]",
                        "dbName": "[parameters('dbName')]",
                        "sqlAdmin": "[parameters('sqlAdmin')]",
                        "sqlPwd": "[parameters('sqlPwd')]",
                        "saKey": "[parameters('saKey')]"
                    }   
                },
                "protectedSettings": {}
            }
        }
    ]
}